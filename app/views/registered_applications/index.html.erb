 <h1>All Registered Applications</h1>

 <div class="row">
   <div class="col-md-8">

     <% @registered_applications.each do |registered_application| %>
     
       <div class="media">
         <div class="media-body">
           <h4 class="media-heading">
             <%= link_to registered_application.name, registered_application %>
             <br/>
             <small>submitted <%= time_ago_in_words(registered_application.created_at) %> ago</small>
           </h4>
    
           <p>
             <%= registered_application.url %> 
           </p>

         </div> <!--end media-body-->
       </div> <!--end media-->
     <% end %>

     </br>
     <%= link_to "Exit", welcome_index_path, class: 'btn btn-warning' %>
   </div>
 </div>
